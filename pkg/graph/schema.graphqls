type Invocation {
  id: ID!
  status: Int!
  method: String!
  url: String!
  requestHeaders: [Header]
  responseHeaders: [Header]
  requestBody: String
  responseBody: String
}

type Header {
  name: String!
  value: String!
}

type Query {
  invocations(status: Int!): [Invocation!]!
}

type Subscription {
  name: String!
}
